    "jobber-auth:prisma": "npm run jobber-auth:generate-types && npm run jobber-auth:migrate-prisma",
    "docker:compose:up": "docker compose up -d",
    "docker:compose:down": "docker compose down",
    "rimraf:dist": "npx rimraf dist",
    "rimraf:node_modules": "npx rimraf node_modules",
    "rimraf:prisma:migrations": "npx rimraf apps/jobber-auth/prisma/migrations",
    "postinstall": "npm run jobber-auth:generate-types"
